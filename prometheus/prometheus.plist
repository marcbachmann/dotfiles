<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>local.marcbachmann.prometheus</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Users/marcbachmann/.dotfiles/prometheus/prometheus</string>
      <string>--web.listen-address=0.0.0.0:9999</string>
      <string>--config.file=/Users/marcbachmann/.dotfiles/prometheus/prometheus.yml</string>
      <string>--storage.tsdb.path=/Users/marcbachmann/.dotfiles/prometheus/data</string>
      <string>--storage.tsdb.retention=360d</string>
      <string>--web.enable-lifecycle</string>
      <string>--web.enable-admin-api</string>
